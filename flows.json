    {
        "id": "function1",
        "type": "function",
        "z": "2c517c7761fb86b7",
        "name": "Decide GPIO",
        "func": "const temp = msg.payload.current.temperature_2m;\n\nlet gpioMsg = {\n    payload: 1\n};\n\nif (temp <= 0) {\n    gpioMsg.pin = 12;\n} else if (temp <= 15) {\n    gpioMsg.pin = 13;\n} else if (temp <= 30) {\n    gpioMsg.pin = 14;\n} else {\n    return null; // no action for >30Â°C\n}\n\nmsg.payload = gpioMsg.payload;\nmsg.topic = gpioMsg.pin.toString();\n\nreturn msg;",
        "outputs": 1,
        "timeout": "",
        "noerr": 0,
        "initialize": "",
